{
  "name": "yobo",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "start": "node ./index.js",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "dev": "NODE_ENV=DEV nodemon ./index.js",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "generate-coverage-report": "npx jest-html-reporter",
    "prepare": "husky install"
  },
  "engines": {
    "node": "20"
  },
  "main": "index.js",
  "dependencies": {
    "africastalking": "^0.6.2",
    "axios": "^1.3.6",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.4.1",
    "formidable-serverless": "^1.1.1",
    "http-status": "^1.6.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemon": "^3.0.1",
    "sha256": "^0.2.0",
    "uuid-v4": "^0.1.0",
    "x-hub-signature": "^2.1.1"
  },
  "private": true,
  "version": "1.0.0",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "husky": "^8.0.3",
    "istanbul": "^0.4.5",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "supertest": "^6.3.3"
  }
}
